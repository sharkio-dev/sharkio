{
  "name": "server",
  "version": "1.0.0",
  "description": "A nodejs proxy that reads all the traffic and makes sense of it",
  "main": "./dist/main.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "nodemon main.ts",
    "dev": "nodemon main.ts",
    "proxytest": "nodemon proxy-test.ts",
    "prod": "node ./dist/main.js",
    "docker:build": "docker build . -t sharkio-server",
    "docker:run": "docker run -p 5012:5012 --name sharkio-sniffer -d sharkio-server",
    "build": "tsc",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "format": "prettier --write \"{,!(node_modules)/**/}*.ts\""
  },
  "nodemonConfig": {
    "ignore": [
      "./sniffers-setup.json",
      "./sniffers-setup-temp.json"
    ]
  },
  "author": "Ido David",
  "license": "ISC",
  "dependencies": {
    "@bany/curl-to-json": "^1.2.8",
    "@supabase/supabase-js": "^2.38.1",
    "axios": "^1.3.4",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-http-proxy": "^1.6.3",
    "express-promise-router": "^4.1.1",
    "http-proxy-middleware": "^2.0.6",
    "jsonwebtoken": "^9.0.2",
    "node-json-db": "^2.1.4",
    "npm": "^9.6.5",
    "openai": "^4.16.1",
    "pg": "^8.11.3",
    "random-string": "^0.2.0",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0",
    "winston": "^3.11.0",
    "yargs": "^17.7.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@swc/core": "^1.3.76",
    "@swc/helpers": "^0.5.1",
    "@swc/jest": "^0.2.29",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-http-proxy": "^1.6.3",
    "@types/jest": "^29.5.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.8.7",
    "@types/random-string": "^0.0.31",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.1",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.2"
  }
}
